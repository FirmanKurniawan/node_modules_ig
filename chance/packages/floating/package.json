{
  "name": "@chancejs/floating",
  "version": "2.2.5",
  "description": "ChanceJS random floating generator",
  "main": "lib/main.js",
  "typings": "lib/main.d.ts",
  "scripts": {
    "build": "tsc",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov --report-dir=./coverage > ./coverage/lcov.info",
    "test": "ava",
    "test:watch": "ava --watch",
    "watch": "tsc --watch"
  },
  "repository": {
    "type": "git",
    "url": "github.com/chancejs/chancejs"
  },
  "keywords": [
    "chance",
    "chancejs",
    "random",
    "bool"
  ],
  "author": "Victor Quinn <mail@victorquinn.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^1.4.1",
    "onchange": "^6.0.0",
    "nyc": "^10.3.2",
    "typescript": "^3.5.3"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [ "ts" ],
    "require": [ "ts-node/register" ],
    "files": [ "src/**/*.test.ts" ]
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@chancejs/integer": "^2.2.5",
    "@types/node": "^12.0.3"
  }
}
